{
    "page": {
        "index": "index.csp",
        "404":"index.csp"
    },
    "service": [
        {
            "name": "mvcservice",
            "option": {
                "database": {
                    "host": "localhost",
                    "port": "3306",
                    "debug": false,
                    "database": "blog",
                    "user": "root",
                    "password": "",
                    "connectionLimit ": "200"
                },
                "view": {
                    "path": "front/src",
                    "suffix": "html"
                }
            }
        }
    ],
    "filter": [
        {
            "name": "mvcfilter",
            "option": {}
        },
        {
            "name": "cachefilter",
            "option": {
                "etag": true,
                "cacheSetting": {
                    "png": 20000,
                    "js": 2000,
                    "default": 2000
                }
            }
        },
        {
            "name": "zipfilter",
            "option": {
                "gzip": "js,css"
            }
        }
    ]
}